<h3>The information on the page is protected. Only authenticated users are able to access this page.</h3>
<div class="container">
  <div class="row">
    <div class="col-8 offset-2">
      <div class="card mt-4">
        <div class="card-body">
          <h5 class="card-title">Enter some sample data here and watch it appear below</h5>
          <form class="data">
            <div class="form-group">
              <label for="inputExampleDataName">Data Name</label>
              <input type="text" class="form-control" id="inputExampleDataName" placeholder="Enter Data Title">
            </div>
            <div class="form-group">
              <label for="inputExampleDataText">Data Text</label>
              <input type="text" class="form-control" id="inputExampleDataText" placeholder="Enter Data Text">
            </div>
            <button type="submit" class="btn btn-primary btn-block" id="form-submit">Submit</button>
          </form>
        </div>
      </div>
    </div>
  </div>
  <hr>
  <div class="card">
    <h3 class="card-title" style="padding:1%;">Submitted Data:</h3>
    <ul id="example-list" class="list-group">
      {{#each examples}}
      <li data-id="{{this.id}}" class="list-group-item">
        <a href="example/{{this.id}}">{{this.text}}</a>
        <button class="btn btn-danger float-right delete">ï½˜</button>
      </li>
      {{/each}}
    </ul>
  </div>
</div>


